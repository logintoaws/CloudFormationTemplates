{
	"AWSTemplateFormateVersion": "2010-09-09",
	"Description": "This template to create An instance",
	"Parameters": {
		"KeyName": {
			"Description": "Key Pair Name",
			"Type": "AWS::EC2::KeyPair::KeyName",
			"Default": "mykey"
		}
		
		"VPC": {
			"Description": " Just select the one and only default VPC",
			"Type": "AWS::EC2::VPC::Id"
		}

		"Subnet": {
			"Description": "Seletct a Subnet",
			"Type": "AWS::EC2::Subnet::Id"
		}
		"InstanceType": {
			"Description": "Select one instance type",
			"Type": "String",
			"Default": "t2.micro",
			"AllowedValues": ["t2.micro","t2.small", "t2.medium"]
		}
		

}
